# React App Environment Variables
# Copy this file to .env and fill in your actual values

# Backend API URL (adjust port if different)
REACT_APP_API_URL=http://localhost:5000

# OpenAI API Key for TTS (Text-to-Speech)
# Get this from https://platform.openai.com/api-keys
REACT_APP_OPENAI_API_KEY=

# Optional: Enable development features
REACT_APP_ENABLE_DEBUG=false

# Optional: Custom app title
REACT_APP_TITLE=AI MCP Assistant

# Optional: SignalR Hub URL (usually same as API URL)
REACT_APP_SIGNALR_URL=http://localhost:5000/notificationHub